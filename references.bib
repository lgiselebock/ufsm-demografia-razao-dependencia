@article{brito_transicao_2008,
  title = {{Transi\c{c}\~ao demogr\'afica e desigualdades sociais no Brasil}},
  author = {Brito, Fausto},
  year = {2008},
  month = jun,
  journal = {Revista Brasileira de Estudos de Popula\c{c}\~ao},
  volume = {25},
  pages = {5--26},
  publisher = {{Associa\c{c}\~ao Brasileira de Estudos Populacionais}},
  issn = {0102-3098, 1980-5519},
  doi = {10.1590/S0102-30982008000100002},
  abstract = {O artigo analisa a originalidade da transi\c{c}\~ao demogr\'afica no Brasil determinada pelos fortes desequil\'ibrios regionais e sociais. Ainda que \'unica, enquanto um processo global que atinge toda a sociedade brasileira, a transi\c{c}\~ao demogr\'afica apresenta-se como m\'ultipla, pois se manifesta diferentemente segundo as diversidades regionais e, principalmente, sociais. Nessa perspectiva, a transi\c{c}\~ao demogr\'afica est\'a longe de ser considerada neutra: pode tanto criar possibilidades demogr\'aficas que potencializem o crescimento da economia e do bem-estar social da popula\c{c}\~ao, quanto ampliar as graves desigualdades sociais que marcam a sociedade brasileira. Essa situa\c{c}\~ao torna-se mais complexa em fun\c{c}\~ao de o Brasil ainda estar inserido no grande ciclo de crescimento absoluto da sua popula\c{c}\~ao. Devido \`as desigualdades sociais e \`as correspondentes diferen\c{c}as nas taxas de fecundidade total, a popula\c{c}\~ao mais pobre \'e a que mais tem crescido, com fortes conseq\"u\^encias sobre as mudan\c{c}as na estrutura et\'aria. As rela\c{c}\~oes entre os diversos indicadores da transi\c{c}\~ao demogr\'afica e a renda domiciliar per capita mostram que as diferen\c{c}as sociais levam, no Brasil, a "desigualdades demogr\'aficas" maiores do que aquelas observadas entre as diferentes regi\~oes. Seus benef\'icios, ou b\^onus demogr\'aficos, s\~ao distintos segundo os n\'iveis sociais. Desse modo, a capacidade de a transi\c{c}\~ao demogr\'afica potencializar as transfer\^encias intergeracionais de recursos est\'a intimamente associada \`a implementa\c{c}\~ao de pol\'iticas que potencializem as transfer\^encias sociais desses mesmos recursos.},
  langid = {portuguese},
  keywords = {Bônus demográfico,Desigualdade social,Transição demográfica},
  file = {C\:\\Users\\luisa\\Zotero\\storage\\TPKK3TLN\\Brito - 2008 - Transição demográfica e desigualdades sociais no B.pdf}
}

@article{carvalho_transicao_2008,
  title = {{A transi\c{c}\~ao da estrutura et\'aria da popula\c{c}\~ao brasileira na primeira metade do s\'eculo XXI}},
  author = {de Carvalho, Jos{\'e} Alberto Magno and {Rodr{\'i}guez-Wong}, Laura L.},
  year = {2008},
  month = mar,
  journal = {Cadernos de Sa\'ude P\'ublica},
  volume = {24},
  number = {3},
  pages = {597--605},
  issn = {0102-311X},
  doi = {10.1590/S0102-311X2008000300013},
  abstract = {A trajet\'oria da popula\c{c}\~ao brasileira, na primeira metade deste s\'eculo, tanto em termos de seu volume, quanto de sua estrutura et\'aria, j\'a est\'a praticamente definida, pois, tanto a transi\c{c}\~ao de mortalidade quanto a da fecundidade j\'a se encontram muito avan\c{c}adas. Enquanto a popula\c{c}\~ao idosa (65 e mais anos de idade) aumentar\'a a taxas altas (entre 2\% e 4\% ao ano), a popula\c{c}\~ao jovem tender\'a a decrescer. Segundo proje\c{c}\~oes das Na\c{c}\~oes Unidas, de 3,1\% da popula\c{c}\~ao total, em 1970, a popula\c{c}\~ao idosa brasileira dever\'a passar a aproximadamente 19\%, em 2050. Paralelamente, conviver\~ao dentro das popula\c{c}\~oes jovem e adulta subgrupos et\'arios com crescimento negativo e positivo. A transi\c{c}\~ao et\'aria brasileira gera oportunidades e desafios que, se n\~ao aproveitados e enfrentados, no momento devido, levar\'a o pa\'is a seri\'issimos problemas, nas pr\'oximas d\'ecadas.           ,              Brazil is currently in an advanced stage of both the mortality and fertility transitions, which allows one to confidently forecast the age distribution and population size over the next four decades. Whereas the elderly population ({$>$} 65 years) will increase at high rates (2-4\% per year), the young population will decline. According to United Nations projections, the elderly population will increase from 3.1\% of the population in 1970 to 19\% in 2050. Meanwhile, within the young and adult populations, growth rates will vary among different age groups, ranging from negative to positive values. The changing age distribution of the Brazilian population brings opportunities and challenges that could lead to serious social and economic issues if not dealt with properly in the coming decades.},
  langid = {portuguese},
  file = {C\:\\Users\\luisa\\Zotero\\storage\\4MLRXVAP\\Carvalho e Rodríguez-Wong - 2008 - A transição da estrutura etária da população brasi.pdf}
}

