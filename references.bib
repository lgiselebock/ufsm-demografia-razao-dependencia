@article{loiola_levantamento_2020,
  title = {{Levantamento de indicadores demogr\'aficos de Santa Maria/RS no per\'iodo de 2000 a 2010}},
  author = {Loiola, Dorietson Braga and Jacobi, Luciane Flores},
  year = {2020},
  journal = {Revista Thema},
  volume = {17},
  number = {4},
  pages = {855--870},
  issn = {2177-2894},
  doi = {10.15536/thema.V17.2020.855-870.1296},
  abstract = {A partir do Banco de Dados disponibilizado pelo Instituto Brasileiro de Geografia e Estat\'istica e de informa\c{c}\~oes coletadas no DATASUS, procurou-se identificar tend\^encias demogr\'aficas e sociais nos anos de estudo, comparando-se, no que diz respeito ao indicador taxa de mortalidade infantil de Santa Maria/RS com o Estado do Rio Grande do Sul, Regi\~ao Sul do Brasil e o Pa\'is como um todo. Al\'em desse indicador, foram calculados e analisados tamb\'em: raz\~ao de sexo, raz\~ao de depend\^encia, idade mediana da popula\c{c}\~ao, taxa bruta de mortalidade, taxa espec\'ifica de mortalidade por sexo e idade, \'indice de sobremortalidade masculina por idade, taxa bruta de natalidade e taxa de fecundidade geral. Como resultado, verificou-se que a popula\c{c}\~ao urbana aumentou, enquanto a rural diminuiu, sendo que as mulheres contribu\'iram mais que os homens em ambos os casos. H\'a predom\'inio da popula\c{c}\~ao feminina nas faixas de idade mais elevadas, sendo que, no meio rural essa superioridade \'e menor. Da mesma forma, houve um aumento na mortalidade, sendo alta nos primeiros anos de vida e a partir da terceira idade, contribuindo mais com ela o sexo masculino, que sofrem um risco bem maior dos 15 aos 49 anos. Palavras-chave: Indicadores sociais; estat\'istica descritiva; popula\c{c}\~ao residente; censo demogr\'afico.},
  copyright = {Copyright (c) 2020 Revista Thema},
  langid = {portuguese},
  keywords = {Censo Demográfico.,Estatística Descritiva,Indicadores Sociais,População Residente},
  file = {C\:\\Users\\luisa\\Zotero\\storage\\IF9CRKGF\\Loiola e Jacobi - 2020 - Levantamento de indicadores demográficos de Santa .pdf}
}

@misc{mendes_o_2021,
  type = {{bachelorThesis}},
  title = {{O impacto da din\^amica populacional nos benef\'icios previdenci\'arios: uma compara\c{c}\~ao a n\'ivel das unidades federativas do brasil de 2012 a 2018}},
  shorttitle = {{O impacto da din\^amica populacional nos benef\'icios previdenci\'arios}},
  author = {Mendes, Lucas Lins},
  year = {2021},
  month = dec,
  abstract = {O presente trabalho tem como objetivo quantificar o impacto da din\^amica populacional, mais precisamente do indicador da raz\~ao de depend\^encia de idosos nas despesas com benef\'icios ativos da previd\^encia social por Unidade Federativa brasileira durante os anos de 2012 a 2018. Para esta pesquisa, foram utilizados dados do Instituto Brasileiro de Geografia e Estat\'istica e do Anu\'ario Estat\'istico da Previd\^encia Social do Minist\'erio da Economia. O modelo utilizado foi o de dados em painel para poder observar as varia\c{c}\~oes tanto no tempo, quanto no espa\c{c}o. Em meio a isto, foram trazidos a origem de cada objeto desse estudo, juntamente com a evolu\c{c}\~ao, atrav\'es de gr\'aficos e tabelas para facilitar a compreens\~ao da rela\c{c}\~ao entre os principais objetos do trabalho, e conceito de seus indicadores. Os resultados mostraram que, assim como a literatura, h\'a, de fato, signific\^ancia estat\'istica para a raz\~ao de depend\^encia de idosos quando utilizada para explicar as varia\c{c}\~oes das despesas com benef\'icios ativos do Regime Geral da Previd\^encia Social.},
  copyright = {openAccess},
  howpublished = {https://repositorio.ufpe.br/handle/123456789/43161},
  langid = {portuguese},
  annotation = {Accepted: 2022-03-04T15:27:17Z},
  file = {C\:\\Users\\luisa\\Zotero\\storage\\X9MWP8QT\\Mendes - 2021 - O impacto da dinâmica populacional nos benefícios .pdf}
